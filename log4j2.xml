<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <patternLayout pattern="%d{HH:mm:ss.SSS} - %msg%n"/>
        </console>
    </appenders>
    <loggers>
        <root level="DEBUG">
            <appenderRef ref="Console"/>
        </root>
    </loggers>
</configuration>